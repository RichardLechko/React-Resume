{
  "name": "my-project",
  "version": "0.1.0",
  "proxy": "http://localhost:5000",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "craco start --config craco.config.cjs",
    "start:backend": "cd backend && node index.js",
    "server": "node backend/index.js",
    "client": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "concurrently": "^8.2.2",
    "customize-cra": "^1.0.0",
    "react-app-rewired": "^2.2.1",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.9",
    "webpack": "^5.94.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.2.0",
    "aws-amplify": "^6.5.3",
    "my-project": "file:",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^6.26.1"
  }
}
